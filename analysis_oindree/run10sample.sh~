#!/bin/bash

#PBS -l walltime=10:00:00
#PBS -l nodes=1:fast:ppn=2
#PBS -j oe
#PBS -o /home/dailey.110/analysis/qsuboutputs/filter_study/

LOCAL_DIR=`pwd`
echo "Local dir is $LOCAL_DIR"

mkdir -p /tmp/$FILTER/$PHASE/$FILENAME

FILENAMER=/tmp/$FILTER/$PHASE/$FILENAME

cd /home/dailey.110/analysis
source /home/dailey.110/.bash_profile

./loop10sample $RUNNUMBER $FILENAMER $FILTER $PHASE

if ((FILTER == 0)) 
then OUTPUT_DIR=no_fill
elif((FILTER ==1))
then OUTPUT_DIR=Abby
elif((FILTER == 2))
then OUTPUT_DIR=wiener
elif((FILTER == 3))
then OUTPUT_DIR=geom_left
elif((FILTER == 4)) 
then OUTPUT_DIR=no_filter
elif((FILTER == 5))
then OUTPUT_DIR=subtraction
fi


echo $OUTPUT_DIR
echo "exited and trying to move file!"

cp /tmp/$FILTER/$PHASE/$FILENAME/output* /data/anita/btdailey/10sample_outputs/$OUTPUT_DIR
#mv $LOCAL_DIR/tmp/$CWDIR/$FILTER/$FILENAME/output* /data/anita/dailey.110/filter_study/saturated/noCW
rm -rf /tmp/$FILTER/$PHASE/$FILENAME


